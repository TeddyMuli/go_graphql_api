type Link {
  id: ID!
  title: String!
  address: String!
  user: User!
}


type User {
  id: ID!
  name: String!
}

type Query {
  link: [Link!]!
}

input NewLink {
  title: String!
  address: String!
}

input RefreshTokenInput {
  token: String!
}

input NewUser {
  username: String!
  password: String!
}

input Login {
  username: String!
  password: String!
}
